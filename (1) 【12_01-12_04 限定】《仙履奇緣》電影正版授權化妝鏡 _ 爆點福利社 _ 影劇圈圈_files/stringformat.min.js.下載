String.format=function(){var c=arguments[0];if(c==null){return""}for(var a=0;a<arguments.length-1;a++){var b=getStringFormatPlaceHolderRegEx(a);c=c.replace(b,(arguments[a+1]==null?"":arguments[a+1]))}return cleanStringFormatResult(c)};String.prototype.format=function(){var a=this.toString();for(var b=0;b<arguments.length;b++){var c=getStringFormatPlaceHolderRegEx(b);a=a.replace(c,(arguments[b]==null?"":arguments[b]))}return cleanStringFormatResult(a)};function getStringFormatPlaceHolderRegEx(a){return new RegExp("({)?\\{"+a+"\\}(?!})","gm")}function cleanStringFormatResult(a){if(a==null){return""}return a.replace(getStringFormatPlaceHolderRegEx("\\d+"),"")};