
function init_fluidvids(){fluidvids.init({selector:["iframe","object"],players:["www.youtube.com","s.yimg.com","movies.yahoo.com","screen.yahoo.com","tw.movies.yahoo.com","tw.screen.yahoo.com","vlog.xuite.net","player.vimeo.com"]})}if(window.jQuery){if(window.fluidvids){init_fluidvids();$(window).ready(function(){init_fluidvids()})}else{$.getScript("/lib/fluidvids/fluidvids.min.js",function(){$(window).load(function(){init_fluidvids()})})}}else{$.getScript("/js/jquery-1.11.1.min.js",function(){init_fluidvids()})};