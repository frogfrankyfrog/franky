function fb_info(){FB.api("/me?fields=email,gender,birthday,name",function(a){loginData.name=a.name;loginData.gender=a.gender;loginData.id=a.id;loginData.birthday=a.birthday;loginData.email=a.email;fb_info2(a.name,a.gender,a.id,a.birthday,a.email)})}
function fb_info2(a,c,d,b,e){$.post(dynamic_site_url+"user/fb_login_tmp",{name:a,gender:c,fb_id:d,birthday:b,email:e},function(a){null==b||null==e?(loginingStatus=!1,swal_function("\u5f88\u62b1\u6b49\uff0c\u60a8\u82e5\u4e0d\u63d0\u4f9bEmail\u6216\u751f\u65e5\uff0c\u5c07\u7121\u6cd5\u7e7c\u7e8c\u9032\u884c\u8a3b\u518a","warning")):fb_login()})};